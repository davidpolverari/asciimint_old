	month = feb,
	year = {2009},
	keywords = {Biomedical curricula, Cognition, Competency evaluation, Expertise, Health professions, Informatics education, Instructional design, Knowledge organization, Learning sciences, Reasoning, Technology-based learning},
	pages = {176--197},
	file = {ScienceDirect Full Text PDF:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/TIFVHJBR/Patel et al. - 2009 - Cognitive and learning sciences in biomedical and .pdf:application/pdf;ScienceDirect Snapshot:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/VMBWAAQ8/S1532046408001500.html:text/html}
