	pages = {345--353},
	file = {ScienceDirect Full Text PDF:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/F3AX48BZ/Proulx et al. - 2005 - Network thinking in ecology and evolution.pdf:application/pdf;ScienceDirect Snapshot:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/82BQT826/S0169534705000881.html:text/html}
}

@article{elzen_socio-technical_1996,
