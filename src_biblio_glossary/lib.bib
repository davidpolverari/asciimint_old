	month = jan,
	year = {2011},
	keywords = {Health informatics, Medicine/Public Health, general},
	pages = {33--42},
	file = {Snapshot:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/PR35BGZH/978-1-84996-441-8_4.html:text/html}
