	author = {Prokopenko, Mikhail and Boschetti, Fabio and Ryan, Alex J.},
	year = {2009},
	keywords = {adaptation, assortativeness, complexity, emergence, entropy rate, excess entropy, Information theory, predictive efficiency, predictive information, self-organization},
	pages = {11--28},
	file = {Full Text PDF:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/CIHA4G4U/Prokopenko et al. - 2009 - An information-theoretic primer on complexity, sel.pdf:application/pdf;Snapshot:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/4SHQ7WG9/abstract.html:text/html}
}

@article{prager_historical_2009,
	title = {Historical and emerging practices in ecological topology},
	volume = {6},
	issn = {1476-945X},
	url = {http://www.sciencedirect.com/science/article/pii/S1476945X08000810},
	doi = {10.1016/j.ecocom.2008.11.001},
	abstract = {“Ecological topology” has recently been highlighted as a “frontier of ecology,” yet the term “ecological topology” only occasionally appears in the literature. On the other hand, the term “topology” appears in a variety of publications in this and other ecologically oriented journals, but its use is varied and applied to a wide cross-section of ecological problems. These variable usages suggest that topology does not have a common meaning to all ecologists. Part of this confusion results from the fact that topological ways of seeing nature are both formally derived from mathematical origins, and informally derived from non-mathematical conceptualizations. Interestingly, parallels occur between both mathematically originated and conceptually originated topology with respect to object-oriented, network topologies (formally derived from Eulerian mathematics), and field-oriented, manifold topologies (formally derived from Poincaréian mathematics). Topological ways of understanding nature and addressing both theoretical and applied problems have served ecology well in the past, but this approach will be improved with a better, more unified understanding among ecologists as to the variety of meanings found and practiced in the science.},
	number = {2},
	urldate = {2014-02-21},
	journal = {Ecological Complexity},
	author = {Prager, Steven D. and Reiners, William A.},
	month = jun,
	year = {2009},
	keywords = {Catastrophe theory, Ecological frontier, Ecology, Networks, Topology},
	pages = {160--171},
	file = {ScienceDirect Full Text PDF:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/CGUJQMN2/Prager and Reiners - 2009 - Historical and emerging practices in ecological to.pdf:application/pdf;ScienceDirect Snapshot:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/RZBJRRTG/S1476945X08000810.html:text/html}
}

@article{manson_simplifying_2001,
