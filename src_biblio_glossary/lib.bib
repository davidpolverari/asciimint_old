	month = jul,
	year = {2014},
	keywords = {Health informatics, Health Services, Information Management, Medical informatics, Statistics for Life Sciences, Medicine, Health Sciences},
	pages = {1--7},
	file = {Full Text PDF:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/292R6UQU/Ückert et al. - 2014 - Past and Next 10 Years of Medical Informatics.pdf:application/pdf;Snapshot:/Users/sei/Library/Application Support/Firefox/Profiles/1k2ne563.default/zotero/storage/II2HCQKU/10.html:text/html}
}

